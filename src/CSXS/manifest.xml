<?xml version='1.0' encoding='UTF-8'?>
<ExtensionManifest 
 ExtensionBundleId="com.my.angular" 
 ExtensionBundleVersion="1.0.0" 
 Version="11.0"
 ExtensionBundleName="Angular Cep Demo"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ExtensionList>
    <Extension Id="com.my.angular.panel" Version="1.0.0" />
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="ILST" Version="[28.0,99.0]" />
    </HostList>
    <LocaleList>
      <Locale Code="All" />
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="11.0" />
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.my.angular.panel">
      <DispatchInfo>
        <Resources>
          <!-- <MainPath>./client/index.html</MainPath> -->
          <MainPath>./client/index.html</MainPath>
          <CEFCommandLine>
            <Parameter>--v=0</Parameter>
            <Parameter>--enable-nodejs</Parameter>
            <Parameter>--mixed-context</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>Panel In Angular</Menu>
          <Geometry>
            <Size>
              <Height>500</Height>
              <Width>350</Width>
            </Size>
          </Geometry>
          <Icons />
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>
